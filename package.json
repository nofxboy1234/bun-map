{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "oxlint --fix",
    "fmt": "oxfmt",
    "tsgo": "bunx --bun tsgo",
    "dev": "bun --hot src/index.tsx",
    "dev:debug": "bun --inspect-brk src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build:static": "bun src/buildStaticFrontend.ts",
    "clean": "rm -rf dist && echo 'Cleaned dist.'"
  },
  "dependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19",
    "@typescript/native-preview": "^7.0.0-dev.20260216.1",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0"
  }
}
