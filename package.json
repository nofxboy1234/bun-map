{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "update:codex": "npm i -g @openai/codex@latest",
    "fmt": "oxfmt",
    "lint": "oxlint --fix",
    "tsgo": "bunx --bun tsgo",
    "dev": "bun --hot src/index.tsx",
    "dev:debug": "bun --inspect-brk src/index.tsx",
    "build": "bun build --target=bun --production --outdir=dist --public-path=/ --env=BUN_PUBLIC_* src/index.tsx",
    "start:dist": "cd dist && GOODBYE=bye! bun index.js",
    "clean": "rm -rf dist && echo 'Cleaned dist.'"
  },
  "dependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19",
    "@typescript/native-preview": "^7.0.0-dev.20260223.1",
    "oxfmt": "^0.34.0",
    "oxlint": "^1.49.0"
  }
}
