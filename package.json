{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "oxlint --fix",
    "fmt": "oxfmt",
    "tsgo": "bunx --bun tsgo",
    "dev": "bun --hot src/index.tsx",
    "dev:debug": "bun --inspect src/index.tsx",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "clean": "rm -rf dist && echo 'Cleaned dist.'",
    "start": "NODE_ENV=production bun src/index.tsx"
  },
  "dependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19",
    "@typescript/native-preview": "^7.0.0-dev.20260207.1",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0"
  }
}
